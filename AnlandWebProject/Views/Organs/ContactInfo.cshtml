                                        <td data-th="課室別" scope="row">第三課 (地價)</td>
                                        <td data-th="主管" class="text-center">課長 </td>
                                        <td data-th="電話" class="text-center">2432-4001</td>
                                        <td data-th="分機" class="text-center">311</td>
                                        <td data-th="傳真" class="text-center">2431-6054</td>
