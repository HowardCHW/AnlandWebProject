                                        <th scope="col" class="text-center">傳真</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td data-th="課室別" colspan="2" scope="row">總機 </td>
                                        <td data-th="電話" class="text-center">2432-4001</td>
                                        <td data-th="分機" class="text-center">&nbsp;</td>
                                        <td data-th="傳真" class="text-center">&nbsp;</td>
