    <script>
        $(document).ready(function () {
            $('#applybirth, #agentbirth').datepicker({
                format: "yyyy-mm-dd",
                autoclose: true,
                startDate: "-100y",
                endDate: "today",
                clearBtn: true,
                calendarWeeks: false,
                todayHighlight: true,
                language: 'zh-TW'
            });

            $('#getappdate').datepicker({
                format: "yyyy-mm-dd",
                autoclose: true,
                startDate: "today",
                clearBtn: true,
                calendarWeeks: false,
                todayHighlight: true,
                language: 'zh-TW'
            });

            $('#appitem1, #appitem2, #appitem3').on('click', function () {
                var checkIDs = $(this).attr('chkID').split(',');
                if ($(this).is(':checked')) {
                    $('#appItemErr').val($(this).val());                    
                    $.each(checkIDs, function (ind, value) {
                        $('#' + value).attr('required', '').attr('data-parsley-required-message', '勾選時必填項目!');
                    });
                } else {
                    $('#appItemErr').val('');
                    $.each(checkIDs, function (ind, value) {
                        $('#' + value).removeAttr('required').removeAttr('data-parsley-required-message');
                        $('#' + value).parsley().validate();
                    });
                }
                $('#appItemErr').parsley().validate();
            });

            $('#purpose5').on('click', function () {
                if ($(this).is(':checked')) {
                    $('#purposenote').removeAttr('disabled');
                    $('#purposenote').attr('required', '').attr('data-parsley-required-message', '選其他時請敘明目的!');
                } else {
                    $('#purposenote').val('').attr('disabled', '');
                    $('#purposenote').removeAttr('required').removeAttr('data-parsley-required-message');
                }
                $('#purposenote').parsley().validate();
            });

            $('#purposeArea input[type=checkbox]').on('click', function () {
                if ($('#purposeArea input[type=checkbox]').is(':checked')) {
                    if ($('#purposeCheck').val() == '') {
                        $('#purposeCheck').val($(this).val());
                    }
                } else {
                    $('#purposeCheck').val('');
                }
                $('#purposeCheck').parsley().validate();
            });

            $('#btnSend').on('click', function () {
                $('#onlineckupForm').parsley().validate();
                if ($('#onlineckupForm').parsley().isValid()) {
                    $('#onlineckupForm').submit();
                }
            });
        });

        window.Parsley.addValidator('verifyuid', {
            requirementType: 'string',
            validateString: function (value, data, parsleyInstance) {
                if (value.search(/^[A-Za-z](1|2)\d{8}$/i) == -1) {
                    return false;
                }

                //建立字母分數陣列(A~Z)
                var city = new Array(
                    1, 10, 19, 28, 37, 46, 55, 64, 39, 73, 82, 2, 11,
                    20, 48, 29, 38, 47, 56, 65, 74, 83, 21, 3, 12, 30
                )
                value = value.toUpperCase();
                //將字串分割為陣列(IE必需這麼做才不會出錯)
                value = value.split('');

                //計算總分
                var total = city[value[0].charCodeAt(0) - 65];
                for (var i = 1; i <= 8; i++) {
                    total += eval(value[i]) * (9 - i);
                }
                //補上檢查碼(最後一碼)
                total += eval(value[9]);
                //檢查比對碼(餘數應為0);
                return ((total % 10 == 0));
            },
            messages: {
                'zh-tw': '身分證格式錯誤!'
            }
        });

        function SuccessMessage(result) {
            if (result) {
                var url = '/OnlineApplication/Status';
                location = url;
            } else {
                ErrorMSG('送出失敗');
            }
        }

        function FailMessage() {
            ErrorMSG('送出失敗');
        }

        /* 去掉字串兩端的空白字元 */
        String.prototype.trim = function () {
            return this.replace(/(^\s+)|(\s+$)/g, "");
        }
    </script>
