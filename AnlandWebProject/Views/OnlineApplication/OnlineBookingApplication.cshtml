            @using (Ajax.BeginForm("OnlineBookingSave", "OnlineApplication", null, new AjaxOptions { HttpMethod = "POST", OnSuccess = "SuccessMessage(data)", OnFailure = "FailMessage" }, new { @id = "onlineckupForm", @class = "m-auto" }))
