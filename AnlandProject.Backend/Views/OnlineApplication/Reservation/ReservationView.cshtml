@model AnlandProject.Service.BusinessModel.OnlineApplicationModel
@{
    ViewBag.Title = "ReservationView";
    Layout = null;
}

<!DOCTYPE html>
<html class="no-js css-menubar" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="bootstrap admin template">
    <meta name="author" content="">

    <title>基隆市安樂地政事務所-網站管理系統</title>

    <link rel="apple-touch-icon" href="~/base/assets/images/apple-touch-icon.png">
    <link rel="shortcut icon" href="~/base/assets/images/apple-touch-icon.png">

    <!-- Stylesheets -->
    <link rel="stylesheet" href="~/global/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/global/css/bootstrap-extend.min.css">

    <!-- Fonts -->
    <link rel="stylesheet" href="~/global/fonts/font-awesome/font-awesome.css">
    <link rel="stylesheet" href="~/base/assets/css/site.css">

    <style>
        @@page {
            margin: 1cm;
            size: A4 portrait;
        }
        .print {
            page-break-after: always;
        }
        @@media print {
            body {
                color: #000;
                font-family: "標楷體";
                padding: 0;
                margin: 0;
            }
            h1,h2,h3,h4,h5 {
                color: #000;
                font-family: "標楷體";
            }
            .printbtn {
                display: none;
            }
            .table-bordered {
                border: 1px solid #000000 !important;
            }
            .table-bordered td,
            .table-bordered th {
                border: 1px solid #000000 !important;
            }
            table.dataTable thead td,
            table.dataTable thead th {
                border-bottom: 2px solid #000000 !important;
            }
            .table-no-bordered {
                border-width: 0 !important;
            }

            .table-no-bordered td,
            .table-no-bordered th {
                border-width: 0 !important;
                padding:0;
            }
        }
        body {
            color: #000;
            padding-top: 20px !important;
            margin: 0;
        }
    </style>

    <!--[if lt IE 9]>
    <script src="global/vendor/html5shiv/html5shiv.min.js"></script>
    <![endif]-->
    <!--[if lt IE 10]>
    <script src="global/vendor/media-match/media.match.min.js"></script>
    <script src="global/vendor/respond/respond.min.js"></script>
    <![endif]-->

</head>

<body>
    <!--[if lt IE 8]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->

    <div class="printbtn text-right">
        <a href="javascript:print();" class="btn btn-primary">列印</a>
    </div>

    <!-- Page -->
    <div class="page-content container-fluid" style="padding: 10px 30px">
        <!-- 內文編輯區 -->
        <div class="panel panel-bordered" style="box-shadow: none">
            <div class="panel-body">

                <table class="table table-no-bordered" style="margin-bottom:0">
                    <tr>
                        <td class="text-center">
                            <h3>基隆市安樂地政事務所-線上預約服務</h3>
                        </td>
                        <td class="text-left" style="width:20%">
                            編號：
                        </td>
                    </tr>
                </table>
                <table class="table table-striped table-bordered" style="margin-bottom:0">
                    <tr>
                        <th width="15%" rowspan="3" class="text-center" style="vertical-align:middle">申請人</th>
                        <td width="10%" class="text-center text-nowrap">姓名</td>
                        <td width="25%">
                            @Model.ApplyName
                        </td>
                        <th width="15%" rowspan="3" class="text-center" style="vertical-align:middle">代理人</th>
                        <td width="10%" class="text-center text-nowrap">姓名</td>
                        <td>
                            @Model.AgentName
                        </td>
                    </tr>
                    <tr>
                        <td width="10%" class="text-center text-nowrap">出生日期</td>
                        <td width="25%">
                            @if (Model.ApplyBirth.HasValue) { @Model.ApplyBirth.Value.ToString("yyyy/MM/dd") }
                        </td>
                        <td width="10%" class="text-center text-nowrap">出生日期</td>
                        <td>
                            @if (Model.AgentBirth.HasValue) { @Model.AgentBirth.Value.ToString("yyyy/MM/dd") }
                        </td>
                    </tr>
                    <tr>
                        <td width="10%" class="text-center text-nowrap">統一編號</td>
                        <td width="25%">
                            @Model.ApplyID
                        </td>
                        <td width="10%" class="text-center text-nowrap">統一編號</td>
                        <td>
                            @Model.AgentID
                        </td>
                    </tr>
                    <tr>
                        <th width="15%" class="text-center" style="background-color:#DDD">聯絡電話</th>
                        <td colspan="2" style="background-color:#DDD">
                            @Model.ApplyTel
                        </td>
                        <th width="15%" class="text-center" style="background-color:#DDD">電子信箱</th>
                        <td colspan="2" style="background-color:#DDD">
                            @Model.ApplyEMail
                        </td>
                    </tr>
                    <tr>
                        <th class="text-center text-nowrap" width="15%">※ 委任關係</th>
                        <td colspan="5">
                            @if (Model.AgentRe == "1")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            本申請案，係受申請人之委託，如有虛偽不實，願負法律責任。
                        </td>
                    </tr>
                </table>
                <table class="table table-striped table-bordered" style="margin-bottom:0">
                    <tr>
                        <td style="background-color:#DDD">
                            <table class="table table-no-bordered" style="margin-bottom:0">
                                <tr style="background-color:rgba(221,221,221,1)">
                                    <td rowspan="3" class="text-nowrap">申請項目：</td>
                                    <td>
                                        @if (Model.AppItem1.HasValue && Model.AppItem1.Value == 1)
                                        {
                                            <i class="icon fa-square" aria-hidden="true"></i>
                                        }
                                        else
                                        {
                                            <i class="icon fa-square-o" aria-hidden="true"></i>
                                        }
                                        複印土地登記申請原案(收件字號： @Model.Year1 年 @Model.DocItem1 字第 @Model.No1 號)，份數 @Model.Copy1
                                    </td>
                                </tr>
                                <tr style="background-color:rgba(221,221,221,1)">
                                    <td>
                                        @if (Model.AppItem2.HasValue && Model.AppItem2.Value == 1)
                                        {
                                            <i class="icon fa-square" aria-hidden="true"></i>
                                        }
                                        else
                                        {
                                            <i class="icon fa-square-o" aria-hidden="true"></i>
                                        }
                                        複印信託專簿(收件字號： @Model.Year2 年 @Model.DocItem2 字第 @Model.No2 號)，份數 @Model.Copy2
                                    </td>
                                </tr>
                                <tr style="background-color:rgba(221,221,221,1)">
                                    <td>
                                        @if (Model.AppItem3.HasValue && Model.AppItem3.Value == 1)
                                        {
                                            <i class="icon fa-square" aria-hidden="true"></i>
                                        }
                                        else
                                        {
                                            <i class="icon fa-square-o" aria-hidden="true"></i>
                                        }
                                        下班領取登記案件(收件字號： @Model.Year3 年 @Model.DocItem3 字第 @Model.No3 號)
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td style="background-color:#DDD">
                            <table class="table table-no-bordered" style="margin-bottom:0">
                                <tr style="background-color:rgba(221,221,221,1)">
                                    <td rowspan="2" class="text-nowrap">申請用途：</td>
                                    <td>
                                        @if (Model.Purpose1.HasValue && Model.Purpose1.Value == 1)
                                        {
                                            <i class="icon fa-square" aria-hidden="true"></i>
                                        }
                                        else
                                        {
                                            <i class="icon fa-square-o" aria-hidden="true"></i>
                                        }
                                        購屋、貸款使用
                                        @if (Model.Purpose2.HasValue && Model.Purpose2.Value == 1)
                                        {
                                            <i class="icon fa-square" aria-hidden="true"></i>
                                        }
                                        else
                                        {
                                            <i class="icon fa-square-o" aria-hidden="true"></i>
                                        }
                                        處理訴訟案件
                                        @if (Model.Purpose3.HasValue && Model.Purpose3.Value == 1)
                                        {
                                            <i class="icon fa-square" aria-hidden="true"></i>
                                        }
                                        else
                                        {
                                            <i class="icon fa-square-o" aria-hidden="true"></i>
                                        }
                                        政府機關申請案
                                        @if (Model.Purpose4.HasValue && Model.Purpose4.Value == 1)
                                        {
                                            <i class="icon fa-square" aria-hidden="true"></i>    
                                        }
                                        else
                                        {
                                            <i class="icon fa-square-o" aria-hidden="true"></i>    
                                        }
                                        自行參考
                                    </td>
                                </tr>
                                <tr style="background-color:rgba(221,221,221,1)">
                                    <td>
                                        @if (!string.IsNullOrWhiteSpace(Model.PurposeNote))
                                        {
                                            if (Model.Purpose5.HasValue && Model.Purpose5.Value == 1)
                                            {
                                                <i class="icon fa-square" aria-hidden="true"></i>
                                            }
                                            else
                                            {
                                                <i class="icon fa-square-o" aria-hidden="true"></i>
                                            }
                                            @Model.PurposeNote
                                        }
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
                <table class="table table-striped table-bordered" style="margin-bottom:0">
                    <tr>
                        <th class="text-center text-nowrap" width="15%">預約領件日期</th>
                        <td>
                            @if (Model.GetAppDate.HasValue) { @Model.GetAppDate.Value.ToString("yyyy/MM/dd") }
                        </td>
                        <th width="15%">時間</th>
                        <td>
                            @Model.GetAppTime
                        </td>
                    </tr>
                    <tr>
                        <th class="text-center text-nowrap" width="15%" style="background-color:#DDD">其他備註事項欄</th>
                        <td colspan="3" style="background-color:#DDD">
                            @Model.OtherNote
                        </td>
                    </tr>
                    <tr>
                        <th class="text-center text-nowrap">注意事項</th>
                        <td colspan="3">
                            <ol>
                                <li>本申請案如委託代理人代為申請，應填明代理人姓名、出生日期及身分證字號，並於委任關係欄勾選切結「本申請案，係受申請人之委託，如有虛偽不實，願負法律責任」。</li>
                                <li> 如非申請複印全案，請於其他備註事項欄個別敘明欲申請複印之文件名稱。</li>
                                <li> 填寫時如有任何問題請洽(02)24324001分機124 </li>
                            </ol>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="4" style="border-bottom-width: 0 !important">
                            <h3>此致 基隆市安樂地政事務所</h3>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="4" style="border-top-width: 0 !important">
                            <table class="table table-no-bordered" style="margin-bottom:0">
                                <tr>
                                    <td class="text-nowrap" width="14%">申請人簽章：</td>
                                    <td width="20%">&nbsp;</td>
                                    <td class="text-nowrap" width="16%">※代理人簽章：</td>
                                    <td width="20%">&nbsp;</td>
                                    <td class="text-nowrap" width="12%">申請日期：</td>
                                    <td> @if (Model.WDate.HasValue) { @Model.WDate.Value.ToString("yyyy/MM/dd") } </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
                <P class="print"></P>
                <table class="table table-no-bordered" style="margin-bottom:0">
                    <tr>
                        <td class="text-center">
                            <h3>線上預約服務網頁問卷</h3>
                        </td>
                    </tr>
                </table>
                <table class="table table-striped table-bordered" style="margin-bottom:0">
                    <tr>
                        <th class="text-left">1.請問您從何處得知本所「線上預約服務」？</th>
                    </tr>
                    <tr>
                        <td>
                            @if (Model.Qa1 == "1")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            傳播媒體
                            @if (Model.Qa1 == "2")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            報章雜誌
                            @if (Model.Qa1 == "3")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            本所宣導品
                            @if (Model.Qa1 == "4")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            親朋好友告知
                            @if (Model.Qa1 == "5")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            自己發現
                            @if (Model.Qa1 == "6")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            其他：
                            @Model.QaNote
                        </td>
                    </tr>
                    <tr>
                        <th class="text-left">2.請問您對於本所「線上預約服務」網頁操作介面是否滿意？</th>
                    </tr>
                    <tr>
                        <td>
                            @if (Model.Qa2 == "1")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            非常滿意
                            @if (Model.Qa2 == "2")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            滿意
                            @if (Model.Qa2 == "3")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            普通
                            @if (Model.Qa2 == "4")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            不滿意
                            @if (Model.Qa2 == "5")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            非常不滿意
                        </td>
                    </tr>
                    <tr>
                        <th class="text-left">3.請問您認為「線上預約服務」此項服務，能節省您多少時間？</th>
                    </tr>
                    <tr>
                        <td>
                            @if (Model.Qa3 == "1")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            4小時以上
                            @if (Model.Qa3 == "2")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            3-4小時
                            @if (Model.Qa3 == "3")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            2-3小時
                            @if (Model.Qa3 == "4")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            1-2小時
                            @if (Model.Qa3 == "5")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            1小時以內
                        </td>
                    </tr>
                    <tr>
                        <th class="text-left">4.總體而言，請問您對於本所「線上預約服務」此項服務，是否滿意？</th>
                    </tr>
                    <tr>
                        <td>
                            @if (Model.Qa4 == "1")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            非常滿意
                            @if (Model.Qa4 == "2")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            滿意
                            @if (Model.Qa4 == "3")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            普通
                            @if (Model.Qa4 == "4")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            不滿意
                            @if (Model.Qa4 == "5")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            非常不滿意
                        </td>
                    </tr>
                    <tr>
                        <th class="text-left">5.請問您的職業是：</th>
                    </tr>
                    <tr>
                        <td>
                            @if (Model.Qa5 == "1")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            地政士或不動產相關行業
                            @if (Model.Qa5 == "2")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            銀行或金融業
                            @if (Model.Qa5 == "3")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            軍公教
                            @if (Model.Qa5 == "4")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            農漁商
                            @if (Model.Qa5 == "5")
                            {
                                <i class="icon fa-square" aria-hidden="true"></i>
                            }
                            else
                            {
                                <i class="icon fa-square-o" aria-hidden="true"></i>
                            }
                            自由業或其他
                        </td>
                    </tr>
                    <tr>
                        <th class="text-left">
                            填寫日期：
                            @if (Model.WDate.HasValue) { @Model.WDate.Value.ToString("yyyy/MM/dd") }
                        </th>
                    </tr>
                </table>
            </div>
        </div>
        <!-- /內文編輯區 -->

    </div>
    <!-- End Page -->
    <!-- Core  -->
    <script src="~/global/vendor/jquery/jquery.js"></script>
    <script src="~/global/vendor/bootstrap/bootstrap.js"></script>
</body>

</html>

